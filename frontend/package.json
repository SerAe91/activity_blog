{
  "name": "activity-blog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "open": "yarn start --open",
    "test": "jest --maxWorkers=50%",
    "test:clear": "yarn test --clearCache",
    "test:coverage": "yarn test --coverage",
    "test:update-snapshots": "yarn test -u",
    "lint": "ng lint",
    "lint:fix": "yarn lint --fix",
    "lint:report": "yarn lint --format json > report/report.json",
    "lint:format-code": "prettier --config .prettierrc --write \"src/**/*.ts\" \"src/**/*.scss\"",
    "e2e": "ng e2e",
    "sonar": "cross-env-shell sonar-scanner -Dsonar.host.url=$IFOPS_SONAR_URL -Dsonar.login=$IFOPS_SONAR_KEY",
    "sonar:prepare": "npm-run-all --continue-on-error test:coverage lint:report",
    "sonar:scan": "npm-run-all sonar:prepare sonar",
    "check-all": "npm-run-all lint:format-code lint:fix test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/material": "^18.2.14",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/router": "^18.2.13",
    "bootstrap": "^5.3.2",
    "bootstrap-scss": "^4.6.1",
    "chokidar": "^3.5.2",
    "rxjs": "^7.8.0",
    "rxjs-compat": "^6.6.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.13",
    "@types/jest": "^29.5.10",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "jest": "^29.6.2",
    "jest-junit": "12.0.0",
    "jest-preset-angular": "^14.4.2",
    "jest-sonar-reporter": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "protractor": "~7.0.0",
    "sonar-scanner": "^3.1.0",
    "typescript": "^5.4.5"
  }
}
